<?xml version="1.0" encoding="UTF-8"?>
<yang-config>
    <base-config>
        <!--是否使用oscp代码生成器-->
        <oscp-resolve>true</oscp-resolve>
        <!--对应集合的包装类-->
        <collection-use>set</collection-use>
        <!--是否生成unit-bean-->
        <oscp-unitbean>false</oscp-unitbean>
        <!--保留包-->
        <reserved-package></reserved-package>
        <!--是否生成代码注释-->
        <java-doc>true</java-doc>
    </base-config>
    <transaction-config>
        <module name="com.zte.sdn.oscp.yang.gen.v1.zte.com.network.Device">
            <enable>true</enable>
            <!--存储类型（本地存储local、分布式存储）-->
            <type>distribute</type>
            <!--一致性（强、最终eventual）-->
            <consistency>strong</consistency>
            <!--隔离性（读已提交和可重复读）read-committed repeatable-read-->
            <isolation>read-committed</isolation>
            <!--是否持久化-->
            <durable>true</durable>
        </module>
    </transaction-config>
    <conflict-config>
        <!--连字符替换符-->
        <hyphen-replace></hyphen-replace>
        <!--点号替换符-->
        <period-replace></period-replace>
        <!--下划线替换符-->
        <underscore-replace></underscore-replace>
        <!--关键字默认前缀-->
        <yang-identify-prefix>
            <java-key-prefix>oscpYangPrefix</java-key-prefix>
        </yang-identify-prefix>
    </conflict-config>
    <node-config>
        <node name="com.zte.sdn.oscp.yang.gen.v1.zte.com.network.Device">
            <replace attrIn="key" attrOut="identify"/>
            <extend-config></extend-config>
        </node>
    </node-config>
</yang-config>
